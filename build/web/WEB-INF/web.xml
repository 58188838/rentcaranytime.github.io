<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Validate</servlet-name>
        <servlet-class>Validate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Welcome</servlet-name>
        <servlet-class>Welcome</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>changePassword</servlet-name>
        <servlet-class>changePassword</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLogin</servlet-name>
        <servlet-class>AdminLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminValidate</servlet-name>
        <servlet-class>AdminValidate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminWelcome</servlet-name>
        <servlet-class>AdminWelcome</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminViewData</servlet-name>
        <servlet-class>AdminViewData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MailApp</servlet-name>
        <servlet-class>MailApp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Validate</servlet-name>
        <url-pattern>/Validate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Welcome</servlet-name>
        <url-pattern>/Welcome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>changePassword</servlet-name>
        <url-pattern>/changePassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/AdminLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminValidate</servlet-name>
        <url-pattern>/AdminValidate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminWelcome</servlet-name>
        <url-pattern>/AdminWelcome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminViewData</servlet-name>
        <url-pattern>/AdminViewData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MailApp</servlet-name>
        <url-pattern>/MailApp</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
